{
  "name": "bootstyle",
  "version": "0.0.0",
  "description": "Bootstyle is a design framework for React Native. The library is inspired by Bootstrap and tries to bring best practices from the Sass/Css world to JavaScript.",
  "author": "Markus Wetzel <markuswetzel@gmx.net>",
  "license": "MIT",
  "keywords": [
    "react",
    "native",
    "react-native",
    "bootstrap",
    "ui",
    "style"
  ],
  "repository": "https://github.com/examunity/bootstyle.git",
  "bugs": {
    "url": "https://github.com/examunity/bootstyle/issues"
  },
  "main": "dist/bootstyle.cjs.js",
  "module": "dist/bootstyle.esm.js",
  "scripts": {
    "start": "cd example && yarn start",
    "build": "packsy build",
    "dev": "packsy dev",
    "prepublishOnly": "packsy validate && packsy build"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "css-to-react-native": "^3.0.0",
    "polished": "^4.1.3",
    "prop-types": "^15.7.2",
    "react-native-popover-view": "^4.1.0",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "packsy": "^0.2.2",
    "react": "17.0.2",
    "react-native": "0.64.3"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-native": ">=0.63.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "packsy pre-commit"
    }
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  }
}
